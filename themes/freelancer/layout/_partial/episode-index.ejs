<section id="episodes" class="episodes" style="margin-top:20px;">
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">
        <h2>All Episodes</h2>
        <hr class="star-primary">
      </div>
    </div>
    <div class="row">
      <% get_posts({count: 100, query: {type:"episode"}}).each(function(post){ %>
        <%- partial('episode-preview', {post: post}) %>
      <% }) %>
    </div>
  </div>
</section>